<%- include('../partials/head') %>
    <%- include('../partials/header') %>
        
        <div class="container mt-5 mb-5">
            <div class="row">
                <h1 class="text-center">Wilkomen!</h1>
            </div>
        </div>
        
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-md-6">
                    <h2>Next Bike API</h2>
                    <ul>
                        <li>Bike station position</li>
                        <li>Bike station name</li>
                        <li>Number of available bikes</li>
                    </ul>
                </div>
                <div class="col-sm-12 col-md-6">
                    <div class="container">
                        <% for (var i = 0; i < mobiStations.length; i++) { %>
                            setBikeMarker(<%=  mobiStations[i].lat %>, <%=  mobiStations[i].lng %>, "<%=  mobiStations[i].name %>", <%=  mobiStations[i].bikes %>);
                        <% } %>
                    </div>
                </div>
                
            </div>
        </div>
        <hr>
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-md-6">
                    <h2>VVO</h2>
                    <ul>
                        <li>Id of adress</li>
                    </ul>
                </div>
                <div class="col-sm-12 col-md-6">
                    <% for (var i = 0; i < vvoData.length; i++) { %>
                        <%=  vvoData[i].place %>, <%=  vvoData[i].id %>
                    <% } %>
                    
                </div>
            </div>
        </div>

    <hr>
        <div class="container">
            <div class="row">
                <div class="col-sm-12 col-md-6">
                    <h2>DVB</h2>
                    <ul>
                        <li>Route planning</li>
                        <li>Different transport methods</li>
                    </ul>
                </div>
                <div class="col-sm-12 col-md-6">
                    <%- dvbData %>
                </div>
            </div>
        </div>


        <%- include('../partials/footer') %>